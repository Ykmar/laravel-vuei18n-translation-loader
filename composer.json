{
  "name": "ykmar/vuei18n-translation-loader",
  "description": "Librairie permettant de charger au sein de vueI18n les traductions venant du PHP.",
  "type": "library",
  "autoload": {
    "psr-4": {
      "Ykmar\\VueI18nTranslationLoader\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ykmar\\VueI18nTranslationLoader\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "authors": [
    {
      "name": "Marc COLLET",
      "email": "contact@marc-collet.fr"
    }
  ],
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0",
    "illuminate/routing": "^10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Ykmar\\VueI18nTranslationLoader\\VueI18nTranslationLoaderServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}
